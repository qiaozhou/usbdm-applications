            <File Id='EclipseCFFiles15_$(var.CW_Ver)'  DiskId='1' Source='Install\CW_10_1\usbdm-cfv1-gdi-debug.dll'/>
            <File Id='EclipseCFFiles16_$(var.CW_Ver)'  DiskId='1' Source='Install\CW_10_1\usbdm-cfvx-gdi-debug.dll'/>

