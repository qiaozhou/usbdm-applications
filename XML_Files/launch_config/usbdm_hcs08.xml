               type="string" />
            <applySetting
               id="net.sourceforge.usbdm.connections.usbdm.useAltBDMClock" value="255"
               type="string" />
            <applySetting
               id="net.sourceforge.usbdm.connections.usbdm.useDebugBuild"
	       value="false" type="boolean" />
            <applySetting
               id="net.sourceforge.usbdm.connections.usbdm.useResetSignal" value="0"
