               <applySetting id="com.freescale.cdt.debug.cw.Embedded
               Download.StandardDownload" value="false" type="boolean"/>

               <applySetting id="com.freescale.cdt.debug.cw.Embedded
               Download.ExecuteTasks" value="false" type="boolean"/> <applySetting
               id="com.freescale.cdt.debug.cw.Embedded
               Download.TaskInitialLaunches" type="list" mode="append"><listValue
               value="true"/></applySetting> <applySetting
               id="com.freescale.cdt.debug.cw.Embedded Download.TaskNames"
               type="list" mode="append"><listValue
               value="ProjectName_${ChipSTID}"/></applySetting> <applySetting
